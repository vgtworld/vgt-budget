<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog
		xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

	<changeSet id="1.0-example-table" author="VGT">
		<createTable tableName="example_table">
			<column name="id" type="int">
				<constraints primaryKey="true" nullable="false"/>
			</column>
			<column name="text_column" type="varchar(50)">
				<constraints nullable="false"/>
			</column>
			<column name="integer_column" type="int">
				<constraints nullable="false"/>
			</column>
			<column name="date_column" type="datetime">
				<constraints nullable="false"/>
			</column>
			<column name="boolean_column" type="boolean" defaultValueBoolean="true"/>
		</createTable>
	</changeSet>

</databaseChangeLog>
